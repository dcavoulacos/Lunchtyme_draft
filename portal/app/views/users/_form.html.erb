<%= simple_form_for(@user) do |f| %>

    <%= f.input :name %><br>
 
    <%= f.input :class_year, collection: 2014..2017 %><br>
  
    <%= f.input :res_college, priority: [ "BR" ], collection: [ "BR", "CC","JE", "ES", "TC", "MC", "SY", "TD", "BK", "PC", "DC", "SM"]%><br>

    <%= f.input :email %><br>

    <%= f.input :phone %><br>
   
    <%= f.input :gender, priority: [ " " ], collection: [ "M", "F", " "]%><br>
 
    <%= f.input :facebook_id %><br>
    
    <%= f.submit %>

<% end %>
