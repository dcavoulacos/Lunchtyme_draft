<%= simple_form_for(@schedule) do |f| %>

	<%= f.hidden_field :day %>
	<strong>Day: </strong> <%= Time.now.strftime("%A (%m/%d)") %>	<br><br>

	<%= f.input :start_time, collection: ["11:00", "11:15", "11:30", "11:45", "12:00", "12:15", "12:30", "12:45", "01:00", "01:15", "01:30", "01:45", "02:00", "02:15", "02:30", "02:45", "03:00"] %><br>

	<%= f.input :end_time, collection: ["11:00", "11:15", "11:30", "11:45", "12:00", "12:15", "12:30", "12:45", "01:00", "01:15", "01:30", "01:45", "02:00", "02:15", "02:30", "02:45", "03:00"] %><br>

	<%= f.input	:location %><br>

	<%= f.submit %>
<% end %>